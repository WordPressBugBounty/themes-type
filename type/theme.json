							}
						},
						"border": {
							"width": "2px"
						}
					}
				}
			},
			"core/separator": {
				"border": {
					"color": "#eeeeee",
					"style": "solid",
					"width": "0 0 1px 0"
				},
				"css": "&:not(.is-style-wide):not(.is-style-dots):not(.alignwide):not(.alignfull){width: 100px;border-bottom-width: 4px;}"
			}
		},
		"color": {
			"text": "#222"
		},
		"elements": {
			"button": {
				"border": {
					"radius": "128px"
				},
				"color": {

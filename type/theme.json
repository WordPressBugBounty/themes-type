				{
					"fluid": false,
					"name": "Normal",
					"size": "16px",
					"slug": "normal"
				},
				{
					"fluid": false,
					"name": "Medium",
					"size": "18px",
					"slug": "medium"
				},
				{
					"fluid": false,
					"name": "Large",
					"size": "20px",
					"slug": "large"
				},
				{
					"fluid": false,
					"name": "Larger",
					"size": "24px",
					"slug": "larger"
				},
				{
					"fluid": false,
					"name": "Extra Large",
					"size": "32px",
					"slug": "x-large"
				},
				{
					"fluid": false,
					"name": "Huge",
					"size": "48px",
					"slug": "huge"
				}
			]
		}
